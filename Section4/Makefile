.PHONY: clean All

All:
	@echo "----------Building project:[ VectorChallenge - Debug ]----------"
	@cd "VectorChallenge" && "$(MAKE)" -f  "VectorChallenge.mk"
clean:
	@echo "----------Cleaning project:[ VectorChallenge - Debug ]----------"
	@cd "VectorChallenge" && "$(MAKE)" -f  "VectorChallenge.mk" clean
